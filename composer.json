{
    "name": "wangzd/laravel-dict",
    "description": "laravel-dict是一个用于管理系统常用的配置的简单封装.提高代码的扩展性，可以易读性",
    "email": "wangzhoudong@foxmail.com",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "wangzhouodng",
            "email": "wangzhoudong@foxmail.com"
        }
    ],
    "require": {
        "php":">=5.5.0"
    },
    "require-dev" : {
         "phpunit/phpunit": "~5.0|~6.0|~7.0"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {"Wangzd\\Dict\\": "src/"}
    },
    "autoload-dev": {
        "psr-4": {
            "Wangzd\\Tests\\Dict\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wangzd\\Dict\\DictServiceProvider"
            ],
            "aliases": {
                "Dict": "Wangzd\\Dict\\Facades\\Dict"
            }
        }
    }
}
